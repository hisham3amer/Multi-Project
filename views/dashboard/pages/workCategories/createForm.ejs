<form action="/dashboard/workCategories/createForm/store" method="post" enctype="application/x-www-form-urlencoded" class="mx-3">
    <div class="row form-group mb-4">
        <div class="col">
            <label for="catName">Category Name</label>
            <input type="text" class="form-control" id="catName" name="catName" value="" placeholder="new category name"
                required>
        </div>
    </div>

    <div class="form-group mb-4">
        <button type="submit" class="btn btn-primary">Add Category</button>
    </div>
</form>

